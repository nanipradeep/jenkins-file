pipeline {
    agent any
    
    environment {
        PATH = "/opt/maven/bin:$PATH"
    }
    stages {
        stage('Git Checkout'){
            steps{
                git "https://github.com/javahometech/myweb"
            }
        }   
        stage('maven Build'){
            steps{
                sh"mvn clean install"
            }
        }     
    }
}
