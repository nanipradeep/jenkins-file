pipeline {
    agent any
    environment {
        PATH = "/opt/maven/bin:$PATH"
    }
    stages {
        stage('Hello') {
            steps {
                git "https://github.com/javahometech/myweb"
            }
        }
        stage("build code") {
            steps {
              sh "mvn clean install"
              sh "mv target/*.war target/myweb.war"
            }
        }
        
        }     
    }
}
