pipeline {
    agent any
    environment {
        PATH = "/opt/maven/bin:$PATH"
    }
    stages {
        stage('Hello') {
            steps {
               git "https://github.com/nanipradeep/hello-world.git"
            }
        }
        stage("build code") {
            steps {
              sh "mvn clean package"
            }
        }
    }
}    
